<AccessoriesWindow>:
    name: "accessories"

    BoxLayout:
        orientation: "vertical"
        padding: 30

        BoxLayout:
            orientation: "vertical"

            Label:
                text: "Accessories"
                font_size: 90
                font_name: "Tahoma" 
                color: "E52761"

        BoxLayout:
            orientation: "vertical"

            GridLayout:
                cols: 2

                Label: 
                    text: "No inlet"
                    font_size: 40
                    font_name: "Tahoma"
                CheckBox:
                    group: "Inlet"
                    on_active: root.checkbox_click(self, self.active, "No inlet")

                Label: 
                    text: "Inward-projecting"
                    font_size: 40
                    font_name: "Tahoma"
                CheckBox:
                    group: "Inlet"
                    on_active: root.checkbox_click(self, self.active, "Inward-projecting")

                Label: 
                    text: "Square-edged"
                    font_size: 40
                    font_name: "Tahoma"
                CheckBox:
                    group: "Inlet"
                    on_active: root.checkbox_click(self, self.active, "Square-edged")

                Label: 
                    text: "Chamfered"
                    font_size: 40
                    font_name: "Tahoma"
                CheckBox:
                    group: "Inlet"
                    on_active: root.checkbox_click(self, self.active, "Chamfered")


        BoxLayout:
            orientation: "horizontal"

            Label:
                text: "Outlet"
                bold: True
                font_size: 50
                font_name: "Tahoma"

            Switch:
