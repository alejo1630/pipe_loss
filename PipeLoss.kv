<IntroWindow>:
    name: "intro"
    
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        spacing: 30
        padding: 30

        BoxLayout: 
            orientation: "horizontal"

            Label:
                text: "Pipe Loss"
                font_size: 120
                font_name: "Tahoma" 
                color: "769CED"
            
            Image:
                source: "Images/Logo.png"


        BoxLayout:
            orientation: "horizontal"
            spacing: 30

            Button: # Fluid Button
                text: "Fluid\nProperties"
                halign: "center"
                bold: True
                on_release: app.root.current = "fluid"

            Button:
                text: "Pipe\nCharacteristics"
                halign: "center"
                bold: True
                on_release: app.root.current = "pipe"

            Button:
                text: "Accessories"
                halign: "center"
                bold: True
                on_release: app.root.current = "accessories"


        BoxLayout: 
            orientation: "horizontal"
            spacing: 30
            padding: 50

            Button: 
                text: "Calculation"
                halign: "center"
                bold: True
                pos_hint: {"center_x": 0.5, "center_y": 0.5}
                size_hint: (0.5, 0.5)
                color: "36E00D"
                font_size: 40
                on_release: 
                    root.calculation()
                    app.root.current = "calculation"

            

<FluidWindow>:
    name: "fluid"

    BoxLayout:
        orientation: "vertical"
        padding: 30

        BoxLayout:
            orientation: "vertical"

            Label: # Name of the App
                text: "Fluid Properties"
                font_size: 90
                font_name: "Tahoma" 
                color: "2DB6FF"


        BoxLayout:
            orientation: "horizontal"

            Label:
                text: "Fluid rate [m3/s]"
                font_size: 50
                font_name: "Tahoma"
                pos_hint: {"center_x": 0.5, "center_y": 0.5}
                size_hint: (0.2, 0.2)

            TextInput:
                id: fluid_rate
                multiline: False
                halign: "right"
                valign: "center"
                font_size: 50
                font_name: "Tahoma"
                pos_hint: {"center_x": 0.5, "center_y": 0.5}
                size_hint: (0.2, 0.35)


        BoxLayout:
            orientation: "vertical"
            
            GridLayout:
                cols: 2             

                Label: 
                    text: "Water"
                    font_size: 40
                    font_name: "Tahoma"
                CheckBox:
                    group: "Fluid"
                    on_active: root.checkbox_click(self, self.active, "Water")

                Label: 
                    text: "Air"
                    font_size: 40
                    font_name: "Tahoma"
                CheckBox:
                    group: "Fluid"
                    on_active: root.checkbox_click(self, self.active, "Air")

                Label: 
                    text: "Glycerine"
                    font_size: 40
                    font_name: "Tahoma"
                CheckBox:
                    group: "Fluid"
                    on_active: root.checkbox_click(self, self.active, "Glycerine")                

                Label: 
                    text: "Oil"
                    font_size: 40
                    font_name: "Tahoma"
                CheckBox:
                    group: "Fluid"
                    on_active: root.checkbox_click(self, self.active, "Oil")

                Label: 
                    text: "Gasoline"
                    font_size: 40
                    font_name: "Tahoma"
                CheckBox:
                    group: "Fluid"
                    on_active: root.checkbox_click(self, self.active, "Gasoline")

                Label: 
                    text: "Other"
                    font_size: 40
                    font_name: "Tahoma"
                CheckBox:
                    group: "Fluid"
                    on_active: 
                        root.checkbox_click(self, self.active, "Other")
                        app.root.current = "other_fluid"  
        

        BoxLayout:
            orientation: "horizontal"
            
            Label:
                id: density_label
                font_size: 40
                font_name: "Tahoma"
            
            Label:
                id: viscosity_label   
                font_size: 40
                font_name: "Tahoma"


        BoxLayout:
            orientation: "vertical"
            padding: 50

            Button:
                text: "<|"
                color: "0DA6E0"
                font_size: 40
                bold: True
                on_release: app.root.current = "intro"
                pos_hint: {"center_x": 0.5, "center_y": 0.5}
                size_hint: (0.2, 0.2)


